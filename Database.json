{
    "name": "Logs",
    "tables":[
        {
            "name": "LogTypes",
            "isEnum": true,
            "enumItems": [
                {
                    "key": "Success",
                    "value": 1
                },
                {
                    "key": "Info",
                    "value": 2
                },
                {
                    "key": "Warning",
                    "value": 3
                },
                {
                    "key": "Error",
                    "value": 4
                }
            ]
        },
        {
            "name": "Logs",
            "columns": [
                {
                    "name": "Date"
                },
                {
                    "name": "Text",
                    "type": "longText"
                },
                {
                    "name": "TypeId"
                }
            ]
        }
    ],
    "views": [
        {
            "name": "LogViews",
            "query": [
                "select",
                "   l.Id,",
                "   l.`Date`,",
                "   l.`Text`,",
                "   l.TypeId,",
                "   lt.`Key` as TypeKey",
                "from Logs l",
                "inner join LogTypes lt",
                "on l.TypeId = lt.Id"
            ]
        }
    ]
}